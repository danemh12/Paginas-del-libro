#include <stdio.h>
/*imprime la tabla Fahrenheit-Celsius
 para fanr= 0, 20, ..., 300*/
 main()
 { 
 int fahr, celsius 
 int lower, upper, steap;
    
    lower=0             
    upper=300
    steap=20
     
     fahr=lower
     while(fahr<=upper){
        celsius=5*(fahr-32)/9;
        printf("%d\t%d\n", fahr. celsius);
        fahr=fahr+steap;
    
